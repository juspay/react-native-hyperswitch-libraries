{
  "name": "react-native-scancard-example",
  "description": "Example app for react-native-hyperswitch-scancard",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "bundle:ios": "react-native bundle --platform ios --dev false --entry-file index.js --bundle-output ios/HyperswitchScancardExample/hyperswitch.bundle",
    "start": "react-native start",
    "pods": "pod-install --quiet",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "react": "17.0.2",
    "react-native": "0.68.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/runtime": "^7.26.10",
    "babel-plugin-module-resolver": "^4.1.0",
    "metro-react-native-babel-preset": "^0.67.0",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0"
  }
}
